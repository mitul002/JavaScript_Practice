let catagory = "bus";

switch(catagory)
{
    case "car":
        console.log("This is a car");
        break;
    case "bus":
        console.log("This is a bus");
        break;
    case "train":
        console.log("This is a train");
        break;
    default:
        console.log("Invalid Input");
        break;
}



let category = 'Truck';
let carType;

switch (category) {
  case 'cycle':
    carType = 'This is a Cycle';
    break;
  case 'Motor Bike':
    carType = 'This is a Motor Bike';
    break;
  case 'Truck':
    carType = 'This is a Truck';
    break;
  default:
    carType = 'Unknown Vehicle Category';
}

console.log(carType);
    